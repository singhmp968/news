<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txt_body"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".G_Admin_LevelAuth">
    <!--
This page must be removed during the integration of the project

-->

    <EditText
        android:id="@+id/edt_url"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter Url"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/edt_heading"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter heading"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/edt_body"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Enter the news"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/btnAdminLevel"
        android:layout_width="match_parent"
        android:layout_height="87dp"
        android:layout_margin="50dp"
        android:editable="false"
        android:text="published news"
        android:textStyle="bold" />
</LinearLayout>